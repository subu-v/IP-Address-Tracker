r\n}\r\n","sass\\abstracts\\_mixins.scss":"@mixin flex($direction, $justify-content, $align-items, $flex-gap) {\r\n  display: flex;\r\n  flex-direction: $direction;\r\n  justify-content: $justify-content;\r\n  align-items: $align-items;\r\n  gap: $flex-gap;\r\n}\r\n\r\n@mixin media-query($breakpoint) {\r\n  @if $breakpoint == tab--landscape {\r\n    @media (max-width: 70em) {\r\n      @content;\r\n    }\r\n  }\r\n\r\n  @if $breakpoint == tab--portrait {\r\n    @media (max-width: 48em) {\r\n      @content;\r\n    }\r\n  }\r\n\r\n  @if $breakpoint == phone {\r\n    @media (max-width: 37.5em) {\r\n      @content;\r\n    }\r\n  }\r\n}\r\n","sass\\main.scss":"@use \"base\" as *;\r\n@use \"abstracts\" as *;\r\n\r\n.tracker {\r\n  width: 100%;\r\n\r\n  &__header {\r\n    background-image: url(../../images/pattern-bg.png);\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    @include flex(column, initial, center, 0);\r\n    padding: 2.4rem;\r\n    position: relative;\r\n    height: 35vh;\r\n\r\n    // @include media-query(tab--portrait) {\r\n    //   padding: 4.8rem 2.4rem;\r\n    // }\r\n  }\r\n  &__primary-heading {\r\n    font-size: 3.2rem;\r\n    color: white;\r\n    font-weight: 500;\r\n    letter-spacing: 1.2px;\r\n    margin-bottom: 3.2rem;\r\n  }\r\n\r\n  &__search {\r\n    display: flex;\r\n    height: 4.8rem;\r\n\r\n    &--input {\r\n      border: none;\r\n      outline: none;\r\n      color: var(--clr-grey--dark);\r\n      font-weight: 500;\r\n      border-top-left-radius: 15px;\r\n      border-bottom-left-radius: 15px;\r\n      padding: 2.4rem;\r\n      width: 48rem;\r\n      font-size: 1.6rem;\r\n\r\n      @include media-query(phone) {\r\n        min-width: calc(60rem - 4.8rem);\r\n        font-size: 2rem;\r\n        padding: 3.2rem;\r\n      }\r\n\r\n      &::placeholder {\r\n        color: var(--clr-grey);\r\n        font-size: 1.6rem;\r\n        letter-spacing: 1px;\r\n\r\n        @include media-query(phone) {\r\n          font-size: 2rem;\r\n        }\r\n      }\r\n\r\n      &:hover {\r\n        cursor: pointer;\r\n      }\r\n    }\r\n\r\n    &--button {\r\n      background-color: black;\r\n      border: none;\r\n      width: 4.8rem;\r\n      border-top-right-radius: 15px;\r\n      border-bottom-right-radius: 15px;\r\n      transition: background-color 0.2s ease-in;\r\n\r\n      @include media-query(phone) {\r\n        padding: 3.2rem;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n      }\r\n      &:hover {\r\n        cursor: pointer;\r\n        background-color: var(--clr-grey--dark);\r\n      }\r\n    }\r\n\r\n    &--icon {\r\n      width: 1.4rem;\r\n      height: 1.4rem;\r\n\r\n      @include media-query(phone) {\r\n        width: 1.8rem;\r\n        height: 1.8rem;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__data-container {\r\n    display: grid;\r\n    grid-auto-flow: column;\r\n    grid-auto-columns: 1fr;\r\n    column-gap: 3.2rem;\r\n    max-width: 110rem;\r\n    background-color: white;\r\n    border-radius: 10px;\r\n    padding: 3.2rem 2.4rem;\r\n\r\n    position: absolute;\r\n    bottom: -7.5rem;\r\n    z-index: 999;\r\n\r\n    @include media-query(tab--landscape) {\r\n      max-width: 90rem;\r\n      column-gap: 1.8rem;\r\n    }\r\n\r\n    @include media-query(tab--portrait) {\r\n      min-width: 75rem;\r\n      grid-auto-flow: row;\r\n      grid-auto-rows: 1fr;\r\n      text-align: center;\r\n      row-gap: 3.2rem;\r\n      bottom: -180px;\r\n      z-index: 1000;\r\n    }\r\n\r\n    @include media-query(phone) {\r\n      min-width: 60rem;\r\n      bottom: -150px;\r\n    }\r\n  }\r\n\r\n  &__data {\r\n    position: relative;\r\n    padding-right: 4.8rem;\r\n\r\n    @include media-query(tab--landscape) {\r\n      padding-right: 2.4rem;\r\n    }\r\n\r\n    @include media-query(tab--portrait) {\r\n      padding-right: 0;\r\n    }\r\n\r\n    &:not(:last-child)::after {\r\n      content: \" \";\r\n      position: absolute;\r\n      top: 10px;\r\n      right: 0;\r\n      width: 1px;\r\n      height: 6.4rem;\r\n      background-color: var(--clr-grey);\r\n\r\n      @include media-query(tab--portrait) {\r\n        display: none;\r\n      }\r\n    }\r\n\r\n    &--heading {\r\n      color: var(--clr-grey);\r\n      font-size: 1.2rem;\r\n      font-weight: 700;\r\n      letter-spacing: 1.5px;\r\n      margin-bottom: 1.2rem;\r\n    }\r\n\r\n    &--return {\r\n      font-size: 2.4rem;\r\n      font-weight: 500;\r\n      color: var(--clr-grey--dark);\r\n\r\n      @include media-query(tab--landscape) {\r\n        font-size: 2.2rem;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__map {\r\n    height: 65vh;\r\n    background-color: var(--clr-grey--dark);\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"acdead104d8b4ba7736fd24aecce6929","cacheData":{"env":{}}}